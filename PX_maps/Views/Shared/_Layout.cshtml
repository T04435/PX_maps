<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title - KFC Maps</title>
	<link href='https://fonts.googleapis.com/css?family=Francois+One|Quicksand:400,700' rel='stylesheet' type='text/css'>
	<link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/d3/3.5.3/d3.min.js"></script>
	<script type="text/javascript" runat="server" src="~/Scripts/topojson.v0.min.js"></script>
		
	@Styles.Render("~/Content/css")
	@Scripts.Render("~/bundles/modernizr")

</head>
<body>
	@Html.Partial("_Dashboard")
	<div id="app-container">
		@RenderBody()
	</div>
	
	@Scripts.Render("~/bundles/jquery")
	@Scripts.Render("~/bundles/bootstrap")
	@Scripts.Render("~/Scripts/Site")
	@RenderSection("scripts", required: false)
	<script>
	$(document).ready(function () {
		var date = new Date();
		var weekday = new Array(7);
		weekday[0] = "Sunday";
		weekday[1] = "Monday";
		weekday[2] = "Tuesday";
		weekday[3] = "Wednesday";
		weekday[4] = "Thursday";
		weekday[5] = "Friday";
		weekday[6] = "Saturday";
		var n = weekday[date.getDay()];

		$("#hour-range").val(date.getHours());
		$("#week-day").val(n);
		fillBox($("#hour-range").val());
	});
	</script>

</body>
</html>
